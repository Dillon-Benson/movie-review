<h1>Movies#all</h1>
<p>Find me in app/views/movies/all.html.erb</p>

<ul>
  <% @movies.each do |movie| %>
    <h2><%= link_to movie.title, controller: 'movies', action: 'show', id: movie.id %><h2>
    <p>Release date: <%= movie.release_date %></p><br />
    <p>Rating: <%= movie.rating %></p><br />
    <p>Genre: <%= movie.genre %></p><br />
  <% end %>
</ul>
